{
  "redirects": [
    {
      "source": "/:path*",
      "has": [
        {
          "type": "host",
          "value": "www.lasbekasi.com"
        }
      ],
      "destination": "https://lasbekasi.com/:path*",
      "permanent": true
    },
    {
      "source": "/tentang",
      "destination": "/tentang-kami",
      "permanent": true
    },
    {
      "source": "/layanan",
      "destination": "/layanan-las-bekasi",
      "permanent": true
    },
    {
      "source": "/portfolio",
      "destination": "/album-bengkel-las-mandiri",
      "permanent": true
    },
    {
      "source": "/kontak",
      "destination": "/kontak-bengkel-las-bekasi",
      "permanent": true
    },
    {
      "source": "/layanan/kanopi",
      "destination": "/layanan-las-bekasi/jasa-pembuatan-kanopi-bekasi",
      "permanent": true
    },
    {
      "source": "/layanan/pagar-besi",
      "destination": "/layanan-las-bekasi/jasa-pembuatan-pagar-besi-bekasi",
      "permanent": true
    },
    {
      "source": "/layanan/teralis",
      "destination": "/layanan-las-bekasi/jasa-pembuatan-teralis-bekasi",
      "permanent": true
    },
    {
      "source": "/layanan/railing-tangga",
      "destination": "/layanan-las-bekasi/jasa-pembuatan-railing-tangga-bekasi",
      "permanent": true
    }
  ],
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        },
        {
          "key": "Referrer-Policy",
          "value": "strict-origin-when-cross-origin"
        }
      ]
    },
    {
      "source": "/assets/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/(.*).js",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/(.*).css",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/(.*).webp",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/(.*).png",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/(.*).jpg",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/sitemap.xml",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/xml"
        },
        {
          "key": "Cache-Control",
          "value": "public, max-age=3600, must-revalidate"
        }
      ]
    }
  ],
  "crons": [
    {
      "path": "/api/cron/update-sitemap",
      "schedule": "0 2 * * *"
    }
  ]
}
